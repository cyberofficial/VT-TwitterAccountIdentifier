{
	"manifest_version": 3,
	"name": "VT-TwitterIdentifier",
	"version": "0.0.1",
	"description": "Mark Official HoloLive Twitter accounts. Stable but early release.",
	"background": {
		"service_worker": "serverice_worker.js"
	  },
	"permissions": ["scripting"],
	"host_permissions": ["*://*.twitter.com/*"],
	"content_scripts": [
		{
			"matches": [
				"*://*.twitter.com/*"
			],
			"js": [
				"db_list.js",
				"TwitterIdentifier.js"
			],
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}